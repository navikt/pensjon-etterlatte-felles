        {{/eq}}

        {{#eq this.endringAvInntekt.grunn.svar.verdi "ANNEN_GRUNN" }}
            {{> eypdfgen/partials_v2/common/opplysning this.endringAvInntekt.annenGrunn}}
        {{/eq}}
