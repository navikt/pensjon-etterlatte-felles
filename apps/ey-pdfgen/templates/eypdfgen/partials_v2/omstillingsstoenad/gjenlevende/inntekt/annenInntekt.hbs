        {{> eypdfgen/partials_v2/common/opplysning this.beloep}}

        {{> eypdfgen/partials_v2/common/opplysning this.endringAvInntekt.fremtidigEndringAvInntekt}}

        {{#eq this.endringAvInntekt.fremtidigEndringAvInntekt.svar.verdi "JA" }}
