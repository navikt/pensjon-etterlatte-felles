<div>
    {{#if soeker.arbeidOgUtdanning.tilbud.svar}}
        <div>
            <h4>
                {{#eq spraak "en"}}
                    Received a job offer
                {{else}}
                    {{#eq spraak "nn"}}
                        Tilbod om jobb
                    {{else}}
                        Tilbud om jobb
                    {{/eq}}
                {{/eq}}
            </h4>
            {{#with soeker.arbeidOgUtdanning.tilbud.svar as | tilbud |}}
                <div>
                    {{> eypdfgen/partials_v2/common/opplysning tilbud.nyttArbeidssted}}

                    {{> eypdfgen/partials_v2/common/datoOpplysning tilbud.ansettelsesdato}}

                    {{> eypdfgen/partials_v2/common/opplysning tilbud.ansettelsesforhold}}

                    {{> eypdfgen/partials_v2/common/opplysning tilbud.arbeidsmengde}}

                    {{#eq tilbud.ansettelsesforhold.svar.verdi "MIDLERTIDIG" }}
                        {{> eypdfgen/partials_v2/common/opplysning tilbud.harSluttdato}}
                    {{/eq}}

                    {{#eq tilbud.ansettelsesforhold.svar.verdi "TILKALLINGSVIKAR" }}
                        {{> eypdfgen/partials_v2/common/opplysning tilbud.harSluttdato}}
                    {{/eq}}

                    {{#eq tilbud.harSluttdato.svar.verdi "JA" }}
                        {{> eypdfgen/partials_v2/common/datoOpplysning tilbud.sluttdato}}
                    {{/eq}}

                    {{> eypdfgen/partials_v2/common/opplysning tilbud.aktivitetsplan}}
                </div>
            {{/with}}
        </div>
    {{/if}}

</div>
